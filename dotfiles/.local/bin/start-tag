#!/bin/sh

sleep 1

xdotool key Super+period
xdotool key Super+4
xdotool key Super+b
xdotool key Super+b
xdotool key Super+comma
xdotool key Super+1

xdotool key Super+2
st &
sleep 0.25
xdotool key Super+Alt+space

xdotool key Super+3
st -e /bin/sh -c 'cat ~/.cache/wal/sequences; ranger' &
sleep 0.25
xdotool key Super+Alt+space

xdotool key Super+1
st -e /bin/sh -c 'cat ~/.cache/wal/sequences; cava; zsh' &
sleep 0.25
st -e /bin/sh -c 'cat ~/.cache/wal/sequences; neofetch; bash' &
sleep 0.25
st -e /bin/sh -c 'cat ~/.cache/wal/sequences; btop' &
sleep 0.25
xdotool key Super+b

librewolf &
